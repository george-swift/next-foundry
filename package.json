{
  "name": "next-foundry",
  "version": "0.1.0",
  "private": true,
  "description": "Production-ready Next.js + TypeScript starter with AI SDK, authentication, email, rate limiting, Prisma, Tailwind CSS, and more.",
  "license": "MIT",
  "homepage": "https://next-foundry.vercel.app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/george-swift/next-foundry.git"
  },
  "bugs": {
    "url": "https://github.com/george-swift/next-foundry/issues"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.12.0",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "db:generate": "prisma generate --schema=./src/prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=./src/prisma/schema.prisma",
    "db:push": "prisma db push --schema=./src/prisma/schema.prisma",
    "db:studio": "prisma studio --schema=./src/prisma/schema.prisma",
    "security:audit": "pnpm audit || true",
    "analyze": "pnpm knip",
    "prepare": "husky",
    "postinstall": "prisma generate --schema=./src/prisma/schema.prisma"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.27",
    "@ai-sdk/react": "^2.0.39",
    "@hookform/resolvers": "^5.2.1",
    "@prisma/client": "^6.15.0",
    "@prisma/extension-accelerate": "^2.0.2",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@react-email/components": "^0.5.2",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.3",
    "@vercel/analytics": "^1.5.0",
    "@vercel/functions": "^3.0.0",
    "ai": "^5.0.39",
    "better-auth": "^1.3.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.18",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.542.0",
    "motion": "^12.23.12",
    "next": "15.5.2",
    "next-themes": "^0.4.6",
    "nuqs": "^2.6.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "react-leaflet": "5.0.0-rc.2",
    "recharts": "3.2.0",
    "resend": "^6.0.3",
    "server-only": "^0.0.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@tailwindcss/postcss": "^4",
    "@types/leaflet": "^1.9.20",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "knip": "^5.63.1",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "prisma": "^6.15.0",
    "tailwindcss": "^4",
    "tsx": "^4.20.5",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5"
  }
}
